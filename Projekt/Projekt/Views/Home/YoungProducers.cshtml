@model IEnumerable<Producer>

<table style="width: 50%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Country</th>
            <th>Year of Foundation</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (Producer p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Country</td>
                <td>@p.YearOfFoundation</td>
            </tr>
        }
    </tbody>
</table>
